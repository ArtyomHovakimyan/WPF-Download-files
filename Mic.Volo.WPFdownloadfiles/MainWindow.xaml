<Window x:Class="Mic.Volo.WPFdownloadfiles.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mic.Volo.WPFdownloadfiles"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="URL" HorizontalAlignment="Left" Margin="56,27,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextUrlS" HorizontalAlignment="Left" Height="23" Margin="121,30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="501" Foreground="#FFC32727" CaretBrush="#FFC71F1F" Background="#FFD4BBBB"/>
        <Button x:Name="BtnDownS" Content="Download" HorizontalAlignment="Left" Margin="642,33,0,0" VerticalAlignment="Top" Width="75" Click="BtnDownS_Click"/>
        <ProgressBar x:Name="PbStatus" HorizontalAlignment="Left" Height="17" VerticalAlignment="Top" Width="501" Margin="121,73,0,0"/>

    </Grid>
</Window>
